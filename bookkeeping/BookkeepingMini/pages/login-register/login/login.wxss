/* pages/login-register/login/login.wxss */
page{
  height: 100%;
background-color: #8CB6FF;
}
image {
  position: absolute;
  left: 0;
  bottom: 0;
  display: block;
  width: 100%;
  height: 100%;
  z-index: -999;
}

.containers{
  position: relative;
  /* width: 800rpx; */
  height: 1280rpx;

}
.refister{
  position: absolute;
  left: 50%;
  top: 45%;
  transform: translate(-50%, -50%);
}
.user{
  display: flex;
  width: 630rpx;
  margin-bottom: 80rpx;
  border: #D4E5FF solid 2rpx;
  border-radius: 20rpx;
}
.password{
  display: flex;
  margin-bottom: 20rpx;
  border: #D4E5FF solid 2rpx;
  border-radius: 20rpx;
}
.user-ico{
  color: #889AA4;
  font-size: 70rpx;
  width: 80rpx;
}
.zc{
  display: flex;
  justify-content: space-between;
  color: #fff;
}
.submit{
  margin-top: 30rpx;
}